   "source": [
    "import csv\n",
    "import json\n",
    "\n",
    "csvfile = open('uber-raw-data-apr14.csv', 'r')\n",
    "jsonfile = open('apri.json', 'w')\n",
    "\n",
    "fieldnames = (\"Date-Time\",\"Lat\",\"Long\",\"Base\")\n",
    "reader = csv.DictReader( csvfile, fieldnames)\n",
    "for row in reader:\n",
    "    json.dump(row, jsonfile)\n",
    "    jsonfile.write('\\n')"
   ]
